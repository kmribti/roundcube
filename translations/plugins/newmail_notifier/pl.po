#: plugins/newmail_notifier/localization/pl_PL.inc:basic
msgid "Display browser notifications on new message"
msgstr "Wyświetlaj powiadomienia o nadejściu nowej wiadomości w przeglądarce"

#: plugins/newmail_notifier/localization/pl_PL.inc:sound
msgid "Play the sound on new message"
msgstr "Odtwarzaj dźwięk o nadejściu nowej wiadomości"

#: plugins/newmail_notifier/localization/pl_PL.inc:desktop
msgid "Display desktop notifications on new message"
msgstr "Wyświetlaj powiadomienia o nadejściu nowej wiadomości na pulpicie"

#: plugins/newmail_notifier/localization/pl_PL.inc:test
msgid "Test"
msgstr "Testuj powiadomienie"

#: plugins/newmail_notifier/localization/pl_PL.inc:title
msgid "New Email!"
msgstr "Nowa wiadomość!"

#: plugins/newmail_notifier/localization/pl_PL.inc:body
msgid "You've received a new message."
msgstr "Nadeszła nowa wiadomość."

#: plugins/newmail_notifier/localization/pl_PL.inc:testbody
msgid "This is a test notification."
msgstr "To jest testowe powiadomienie."

#: plugins/newmail_notifier/localization/pl_PL.inc:desktopdisabled
msgid "Desktop notifications are disabled in your browser."
msgstr "Powiadomienia na pulpicie zostały zablokowane w twojej przeglądarce."

#: plugins/newmail_notifier/localization/pl_PL.inc:desktopunsupported
msgid "Your browser does not support desktop notifications."
msgstr "Twoja przeglądarka nie obsługuje powiadomień na pulpicie."
